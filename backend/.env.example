# Aptos Configuration
APTOS_NETWORK=testnet
APTOS_NODE_URL=https://fullnode.testnet.aptoslabs.com/v1
APTOS_FAUCET_URL=https://faucet.testnet.aptoslabs.com

# Facilitator Wallet
FACILITATOR_PRIVATE_KEY=your_private_key_here
FACILITATOR_ADDRESS=your_address_here

# Smart Contract Addresses
ORDER_BOOK_ADDRESS=your_order_book_address
VAULT_ADDRESS=your_vault_address
MODULE_ADDRESS=your_module_address

# Server Configuration
PORT=3001
NODE_ENV=development

# Redis Configuration (optional, for nonce tracking)
REDIS_URL=redis://localhost:6379
ENABLE_REDIS=false

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

